        <activity android:name="com.xprime.FlashLight.FlashLight"
        android:label="@string/flashlight_label"></activity>
        <activity android:name="com.xprime.FlashLight.FlashLight_Setting"
        android:label="@string/flashlight_label"></activity>
        <activity android:name="com.xprime.Compass.Compass"
        android:label="@string/compass_label"></activity>
        <activity android:name="com.xprime.unitconvert.unitconvert"
        android:label="@string/unitconvert_label"></activity>
        <activity android:name="com.xprime.iCalc.iCalc"
        android:label="@string/calculator_label"></activity>
        <activity android:name="com.xprime.SMSSending.SMSSending" 
        android:label="@string/smssending_label"></activity>
        
        

    </application>
</manifest>